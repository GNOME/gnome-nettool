         xmlns="http://usefulinc.com/ns/doap#">

  <name xml:lang="en">gnome-nettool</name>
  <shortdesc xml:lang="en">
    Graphical front-ends to various networking command-line
    tools, like ping, netstat, ifconfig, whois, traceroute, finger.
  </shortdesc>
  
  <homepage rdf:resource="http://projects.gnome.org/gnome-network" />
  <mailing-list rdf:resource="http://mail.gnome.org/mailman/listinfo/gnome-network-list" />
  <download-page rdf:resource="http://download.gnome.org/sources/gnome-nettool/" />
  <bug-database rdf:resource="http://bugzilla.gnome.org/browse.cgi?product=gnome-nettool" />
  <category rdf:resource="http://api.gnome.org/doap-extensions#desktop" />
